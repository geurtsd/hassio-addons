{
    "name": "xComfort-3rd",
    "version": "0.61.2.1",
    "slug": "xcomfort-3rd",
    "description": "Eaton xComfort to (3rd party) MQTT gateway",
    "url": https://github.com/geurtsd/hassio-addons
    "startup": "application",
    "boot": "auto",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "privileged": [
        "SYS_RAWIO"
    ],
    "usb": true,
    "init": false,
    "map": [
        "config:rw"
    ],
    "services": [
        "mqtt:need"
    ],
    "options": {
        "mqtt_client_id": "xcomfort",
        "mqtt_host": "core_mosquitto",
		"mqtt_port": "1883",
        "mqtt_user": "",
        "mqtt_passwd": "",
        "datapoints_file": "",
        "eprom": true,
        "ha_discovery": true,
        "ha_discovery_prefix": "homeassistant",
        "ha_discovery_remove": false,
        "verbose": false,
        "use_hidapi": false,
        "eci_hosts": []
    },
    "schema": {
        "mqtt_client_id": "str",
        "mqtt_host": "str",
		"mqtt_port": "str",
        "mqtt_user": "str",
        "mqtt_passwd": "pwd",
        "datapoints_file": "str",
        "eprom": "bool",
        "ha_discovery": "bool",
        "ha_discovery_prefix": "str",
        "ha_discovery_remove": "bool",
        "verbose": "bool",
        "use_hidapi": "bool",
        "eci_hosts": ["str"]
    }
}
